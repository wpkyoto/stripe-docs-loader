{
  "name": "langchain-stripe-loader",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/langchain-stripe-loader.cjs",
  "module": "./dist/langchain-stripe-loader.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/langchain-stripe-loader.js",
      "require": "./dist/langchain-stripe-loader.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@langchain/core": "^0.3.42",
    "node-html-markdown": "^1.3.0",
    "stripe-loaders-core": "^0.0.0"
  },
  "keywords": [
    "stripe",
    "documentation",
    "loader",
    "langchain",
    "llm",
    "ai",
    "document-loader"
